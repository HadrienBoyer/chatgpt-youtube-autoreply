                snippet=3D<span class=3D"hljs-built_in">dict</span>(
                    parentId=3Dcomment[<span class=3D"hljs-string">"id"</sp=
an>],
                    textOriginal=3Dresponse
                )
            )
        ).execute()
        logging.info(<span class=3D"hljs-string">f"Replied to <span class=
=3D"hljs-subst">{comment[<span class=3D"hljs-string">'id'</span>]}</span> w=
ith: <span class=3D"hljs-subst">{response}</span>"</span>)
        replied_to.add(comment[<span class=3D"hljs-string">"id"</span>])
        num_replies +=3D <span class=3D"hljs-number">1</span>
<span class=3D"hljs-keyword">else</span>:
    <span class=3D"hljs-comment"># If the comment does not meet the require=
ment, add a description to the video</span>
    youtube.videos().update(
