def process_comments(youtube, replied_to, reply_message, video_id):
    # Set up the parameters for the API call
    params =3D {
        "part": "snippet",
        "videoId": video_id,
        "textFormat": "plainText",
        'maxResults': 100,
        'order': 'time',
        'searchTerms': 'Q:'
    }
       =20
    request =3D youtube.commentThreads().list(**params)

    # Loop through the pages of results
    while request:
        # Execute the API call
        response =3D request.execute()
        end =3D False
        # Loop through the comments in the current page of results
        for item in response["items"]:
            comment =3D item["snippet"]["topLevelComment"]
            # Check if the comment has not been replied to yet
            if comment["id"] not in replied_to:
                author_name =3D comment["snippet"]["authorDisplayName"]
                comment_text =3D comment["snippet"]["textDisplay"]
                logging.info(f"Comment from {author_name}: {comment_text}")
               =20
                # Reply to the comment
                youtube.comments().insert(
                    part=3D"snippet",
                    body=3Ddict(
                        snippet=3Ddict(
                            parentId=3Dcomment["id"],
                            textOriginal=3Dreply_message
                        )
                    )
                ).execute()
               =20
                replied_to.add(comment["id"])
            else:
                end =3D True
                break
        if end:
            break
        # Check if there are more pages of results
        if "nextPageToken" in response:
            # Set up the parameters for the next page of results
            request =3D youtube.commentThreads().list(
                **params,
                pageToken=3Dresponse["nextPageToken"]
            )
        else:
            # If there are no more pages of results, exit the loop
            request =3D None
